<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>
    <PREFIX>
        <NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
        <LABEL>rdf</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
        <LABEL>rdfs</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
        <LABEL>foaf</LABEL>
    </PREFIX>    
    <PREFIX>
        <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
        <LABEL>owl</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
        <LABEL>dbpedia</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://purl.org/dc/terms/</NAMESPACE>
        <LABEL>dc</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://purl.org/linked-data/cube#</NAMESPACE>
        <LABEL>qb</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://dbpedia.org/ontology/</NAMESPACE>
        <LABEL>dbo</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://www.w3.org/2004/02/skos/core#</NAMESPACE>
        <LABEL>skos</LABEL>
    </PREFIX>

    <SOURCE>
        <ID>fao-location</ID>
        <ENDPOINT>http://fao.270a.info/sparql</ENDPOINT>
        <GRAPH>http://fao.270a.info/graph/meta</GRAPH>
        <VAR>?x</VAR>
        <PAGESIZE>-1</PAGESIZE>
        <RESTRICTION>?x a skos:Concept</RESTRICTION>
        <RESTRICTION>?x skos:topConceptOf &lt;http://fao.270a.info/code/1.0/CL_SPECIES&gt;</RESTRICTION>
        <PROPERTY>skos:prefLabel AS lowercase->nolang</PROPERTY>
    </SOURCE>

    <TARGET>
        <ID>dbpedia-species</ID>
        <ENDPOINT>http://dbpedia.org/sparql</ENDPOINT>
        <VAR>?y</VAR>
        <PAGESIZE>-1</PAGESIZE>
        <RESTRICTION>?y a dbo:Species</RESTRICTION>
        <PROPERTY>rdfs:label AS lowercase->nolang</PROPERTY>
    </TARGET>

    <METRIC>trigrams(x.skos:prefLabel, y.rdfs:label)</METRIC>

    <ACCEPTANCE>
        <THRESHOLD>0.9</THRESHOLD>
        <FILE>fao-dbpedia.species.trigrams.accept.9.nt</FILE>
        <RELATION>&lt;http://www.w3.org/2004/02/skos/core#exactMatch&gt;</RELATION>
    </ACCEPTANCE>
    <REVIEW>
        <THRESHOLD>0.7</THRESHOLD>
        <FILE>fao-dbpedia.species.trigrams.review.7.nt</FILE>
        <RELATION>&lt;http://www.w3.org/2004/02/skos/core#closeMatch&gt;</RELATION>
    </REVIEW>

    <EXECUTION>Simple</EXECUTION>
    <GRANULARITY>16</GRANULARITY>
    <OUTPUT>N3</OUTPUT>
</LIMES>
